<thlogic>
    <attr sel="#headId"
          th:replace="~{fragments/head :: headFragment}"/>
    <attr sel="#footerId"
          th:replace="~{fragments/footer :: footerFragment}"/>
    <attr sel="#addCarId"
          th:href="${addCar}"
          th:with="addCar=${T(org.lukaszse.carRental.util.Mappings).ADD_CAR}"/>
    <attr sel="#home"
          th:href="${home}"
          th:with="home=${T(org.lukaszse.carRental.util.Mappings).HOME}"/>
</thlogic>
